@model MISAssist.Models.印表機
@{
    ViewBag.Title = "_Details";
}
<!-- Modal content-->
<div class="modal-header modal-header-primary">
    <button type="button" class="close" aria-label="Close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">印表機［詳細］</h4>
</div>
<div id="detailBody" class="modal-body">
    <dl class="dl-horizontal">
        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.使用單位)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.使用單位)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.廠牌)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.廠牌)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.型號)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.型號)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.類別)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.類別)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.購買廠商)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.購買廠商)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.價格)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.價格)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.購買日期)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.購買日期)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.IP位址)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.IP位址)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.碳粉匣_黑)
        </dt>

        <dd style="color:black;font-weight:bold">
            @Html.DisplayFor(model => model.碳粉匣_黑)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.碳粉匣_青)
        </dt>

