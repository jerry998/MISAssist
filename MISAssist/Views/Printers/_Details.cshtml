        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.碳粉匣_黃)
        </dt>

        <dd style="color:orange;font-weight:bold">
            @Html.DisplayFor(model => model.碳粉匣_黃)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.備註)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.備註)
        </dd>

    </dl>
</div>
<div class="modal-footer modal-footer-primary">
    @Html.ActionLink("編輯", "_Edit", new { id = @Html.DisplayFor(model => model.ID) }, new { @class = "btn btn-default", data_target = "#editModal", data_toggle = "modal" })
    @*Html.ActionLink("返回", "Index", new { dept = @Html.DisplayFor(model => model.使用單位) }, new { @class = "btn btn-default" }) *@
    <button type="button" class="btn btn-default" aria-label="Close" data-dismiss="modal">返回</button>
</div>
